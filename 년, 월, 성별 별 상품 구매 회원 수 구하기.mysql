SELECT year(ons.SALES_DATE) AS YEAR, month(ons.SALES_DATE) AS MONTH,
        ui.GENDER AS GENDER, count(DISTINCT ons.USER_ID) AS USERS
FROM ONLINE_SALE ons JOIN USER_INFO ui
ON ons.USER_ID = ui.USER_ID
WHERE ui.GENDER is not null
GROUP BY YEAR, MONTH, GENDER
ORDER BY YEAR, MONTH, GENDER
